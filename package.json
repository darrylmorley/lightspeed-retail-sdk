{
  "name": "lightspeed-retail-sdk",
  "version": "3.0.0",
  "description": "Another unofficial Lightspeed Retail API SDK for Node.js",
  "type": "module",
  "main": "index.cjs",
  "module": "index.mjs",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "default": "./index.mjs"
    }
  },
  "scripts": {
    "test": "node test-sdk.mjs",
    "test:storage": "node test-storage.mjs",
    "test:cjs": "node test-cjs.cjs",
    "test:integration": "node test-integration.mjs",
    "test:all": "npm run test && npm run test:storage && npm run test:cjs"
  },
  "author": "https://github.com/darrylmorley",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.4",
    "dotenv": "^16.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darrylmorley/lightspeed-retail-sdk.git"
  },
  "keywords": [
    "lightspeed retail",
    "node",
    "oauth",
    "api"
  ]
}